{
	"Serilog": {
		"Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"Microsoft": "Information",
				"System": "Information"
			}
		},
		"ElasticConfiguration": {
			"Uri": "http://Admin:Password1!@localhost:9200"
		},
		"Enrich": [ "FromLogContext" ],
		"WriteTo": [
			{ "Name": "Console" },
			{ "Name": "Debug" },
			{
				"Name": "File",
				"Args": {
					"path": "Logs\\log-.txt",
					"rollingInterval": "Day",
					"shared": true
				}
			}
		],
		"Properties": {
			"Application": "TTDoc"
		}
	}
}
